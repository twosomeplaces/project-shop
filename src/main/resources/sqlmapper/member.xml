<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="project.shop.dao.MemberDao">

    <insert id="save" parameterType="project.shop.vo.MemberVo">
        insert into member 
        values (member_seq.NEXTVAL , #{mId}, #{mPw}, #{mName}, #{mBirth}, #{mNum}, #{roles})
    </insert>

    <select id="findOne" parameterType="String" resultType="project.shop.vo.MemberVo">
        select * from member where mId = #{mId}
    </select>

    <select id="findAll" resultType="project.shop.vo.MemberVo">
        select * from member
    </select>

</mapper>